{
    "version": 1,
    "author": "Minimal Example",
    "editor": "wokwi",
    "parts": [
      {
        "type": "board-esp32-devkit-c-v4",
        "id": "esp",
        "top": 0,
        "left": 0,
        "attrs": { "flashSize": "16" }
      }
